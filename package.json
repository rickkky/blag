{
  "name": "vectrix",
  "version": "0.2.1",
  "description": "A easy-to-use math library to perform vector and matrix operations in JavaScript/TypeScript, designed for the needs of graphics apis like WebGL or WebGPU.",
  "keywords": [
    "math",
    "linear-algebra",
    "vector",
    "matrix",
    "webgl",
    "webgpu",
    "computer-graphics",
    "blas"
  ],
  "author": "rickkky <rickkky@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickkky/vectrix.git"
  },
  "homepage": "https://github.com/rickkky/vectrix",
  "bugs": {
    "url": "https://github.com/rickkky/vectrix/issues"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/vectrix.umd.cjs",
  "module": "./dist/vectrix.js",
  "types": "./dist/vectrix.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vectrix.js",
      "require": "./dist/vectrix.umd.cjs",
      "types": "./dist/vectrix.d.ts"
    }
  },
  "scripts": {
    "dev": "vitest",
    "build": "tsc && vite build",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "preview": "vite preview"
  },
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf",
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@vitest/coverage-c8": "^0.33.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.30.1"
  }
}
